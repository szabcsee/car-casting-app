<p id="notice"><%= notice %></p>

<h1>Vehicle Extras</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Vehicle type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vehicle_extras.each do |vehicle_extra| %>
      <tr>
        <td><%= vehicle_extra.name %></td>
        <td><%= vehicle_extra.vehicle_type %></td>
        <td><%= link_to 'Show', vehicle_extra %></td>
        <td><%= link_to 'Edit', edit_vehicle_extra_path(vehicle_extra) %></td>
        <td><%= link_to 'Destroy', vehicle_extra, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vehicle Extra', new_vehicle_extra_path %>
